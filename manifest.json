{
  "name": "Oldness",
  "description": "A Google Chrome extension to inform you how old is the page. Built with Wayback Machine API.",
  "version": "0.0",
  "browser_action": {
    "default_title": "Oldness",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "zepto.min.js",
      "moment.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://archive.org/"
  ],
  "manifest_version": 2
}
